<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>

  <p>
    <strong>Unit price:</strong>
    <%= product.unit_price %>
  </p>

  <p>
    <strong>Supplier:</strong>
    <%= product.supplier_id %>
  </p>

</div>
